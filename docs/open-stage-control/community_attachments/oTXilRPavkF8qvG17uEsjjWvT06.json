        "onValue": "const note = get(\"note_to_map\")\n\nif (get(\"map_note\") && note !== \"none\") {\n  const drumMap = get(\"drum_pad_map\")\n  const padId = getProp(this, \"id\").split(\"_\")[1] - 1\n  drumMap[padId] = note\n  set(\"drum_pad_map\", drumMap)\n  set(\"note_to_map\", \"none\")\n}\nif (get(\"map_note\")) return\nif (value > 0) {\n  locals.playedNote = value\n  send(\"midi:oscCOMMON\", \"/note\", 1, value, 100)\n} else send(\"midi:oscCOMMON\", \"/note\", 1, locals.playedNote, 0)"
