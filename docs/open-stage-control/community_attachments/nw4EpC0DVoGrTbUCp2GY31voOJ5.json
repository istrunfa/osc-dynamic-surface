                "target": "",
                "ignoreDefaults": false,
                "bypass": false,
                "onCreate": "",
                "onValue": "var timerInterval;\nvar timeRemaining;\nvar isCountingUp;\nvar totalSeconds;\n\nfunction startTimer() {\n    const minutes = parseInt(get(\"minutesInput\").value);\n    const direction = get(\"countDirection\").value;\n    console.log(minutes)\n    console.log(direction)\n    if (isNaN(minutes) || minutes <= 0) {\n        send(\"warn\", \"Introdueix un número vàlid de minuts.\");\n        return;\n    }\n\n    isCountingUp = (direction === 'CountUp');\n    totalSeconds = minutes * 60;\n    timeRemaining = isCountingUp ? 0 : totalSeconds;\n\n    updateDisplay();\n    clearInterval(timerInterval);\n    timerInterval = setInterval(count, 1000);\n}\n\nfunction count() {\n    if (isCountingUp) {\n        timeRemaining++;\n        if (timeRemaining >= totalSeconds) {\n            clearInterval(timerInterval);\n            send(\"info\", \"Temps complet!\");\n        }\n    } else {\n        timeRemaining--;\n        if (timeRemaining <= 0) {\n            clearInterval(timerInterval);\n            send(\"info\", \"Temps esgotat!\");\n        }\n    }\n    updateDisplay();\n}\n\nfunction pauseTimer() {\n    clearInterval(timerInterval);\n}\n\nfunction resetTimer() {\n    clearInterval(timerInterval);\n    timeRemaining = isCountingUp ? 0 : totalSeconds;\n    updateDisplay();\n}\n\nfunction updateDisplay() {\n    const minutes = Math.floor(timeRemaining / 60);\n    const seconds = timeRemaining % 60;\n    set(\"timerDisplay\", `${String(minutes).padStart(2, '0')}:${String(seconds).padStart(2, '0')}`);\n}\n",
                "onKeyboard": "",
                "keyBinding": "",
                "keyRepeat": true,
                "keyType": "keydown"
