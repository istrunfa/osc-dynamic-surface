                "onCreate": "",
                "onValue": "if (value.includes(\"f0 43 4f 4c 51 \")) {\n  \n  // Logge die Rohwerte der MIDI-Nachricht\n  //console.log(\"Empfangene MIDI-Nachricht:\", value);\n\n  // MIDI-Nachricht bei Leerzeichen trennen und in Dezimalwerte umwandeln\n  let hexValues = value.split(\" \");  // Die Werte bei Leerzeichen trennen\n  let rHex = hexValues[5];  // R-Wert (Hex)\n  let gHex = hexValues[6];  // G-Wert (Hex)\n  let bHex = hexValues[7];  // B-Wert (Hex)\n\n  //console.log(\"R Hex:\", rHex, \"G Hex:\", gHex, \"B Hex:\", bHex);\n\n  // Überprüfe, ob die Hex-Werte gültig sind\n  let r = parseInt(rHex, 16)*2;  // Hex in Dezimal umwandeln\n  let g = parseInt(gHex, 16)*2;  // Hex in Dezimal umwandeln\n  let b = parseInt(bHex, 16)*2;  // Hex in Dezimal umwandeln\n  \n  //console.log(\"R Dezimal:\", r, \"G Dezimal:\", g, \"B Dezimal:\", b);\n\n  // Wenn r, g, oder b NaN sind, logge eine Fehlermeldung\n  if (isNaN(r) || isNaN(g) || isNaN(b)) {\n    //console.log(\"Fehler: Ungültiger RGB-Wert.\");\n  } else {\n    // Setze die Farbe, wenn die Werte gültig sind\n\n    let print = `rgb(${r}, ${g}, ${b})`;\n    setVar(`selectedTrack_fader`, \"color\", print);\n  \n     for (var i = 1; i<=8;i++){\n    setVar(\"button_\"+i, \"color\", `rgb(${r}, ${g}, ${b})`);// Sends OnOff\n    setVar(\"button_\"+(i+8), \"color\", \"darkgrey\"); // Sends PrePost\n\n    setVar(\"faderB_\"+i, \"color\", `rgb(${r}, ${g}, ${b})`);\n    \n    }\n  }\n}\n",
                "onKeyboard": "",
                "keyBinding": "",
                "keyRepeat": true,
                "keyType": "keydown"
