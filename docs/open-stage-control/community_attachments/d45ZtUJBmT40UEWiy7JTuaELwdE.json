                "ignoreDefaults": false,
                "bypass": false,
                "onCreate": "",
                "onValue": "\nvar num = getProp('parent', 'variables').n;\nvar displayIdx = num;\nvar displayIdxHex = displayIdx.toString(16).padStart(2, '0');\n\nif (value.includes(\"f0 43 4f 4c 52 \" + displayIdxHex)) {\n //console.log(\"value: \"+value+  \" displayIdx: \"+displayIdx + \" displayIdxHex: \"+displayIdxHex)\n\n  let hexValues = value.split(\" \");  \n  let rHex = hexValues[6];  \n  let gHex = hexValues[7];  \n  let bHex = hexValues[8];  \n\n  let r = parseInt(rHex, 16)*2;  \n  let g = parseInt(gHex, 16)*2;  \n  let b = parseInt(bHex, 16)*2;  \n  let printRGB1\n  //console.log('At displayIdx=' + displayIdxHex +  \" r=\" +  r+ \" g=\"+ g+ \" b= \"+ b);\n\n  if (isNaN(r) || isNaN(g) || isNaN(b)) {\n    console.log(\"Fehler: Ungültiger RGB-Wert.\");\n  } else {\n    let print = `rgb(${r}, ${g}, ${b})`;\n    setVar(\"button_track_\"+num, \"color\", `rgb(${r}, ${g}, ${b})`);\n    \n    //if (get(\"switch_B\")===1){\n    //setVar(\"fader_\"+num, \"color\", `rgb(${r}, ${g}, ${b})`);\n      //  setVar(\"button_mixerSelect_\"+num, \"color\", `rgb(${r}, ${g}, ${b})`);\n  //} \n  \n}\n  }\n",
                "onKeyboard": "",
                "keyBinding": "",
                "keyRepeat": true,
                "keyType": "keydown"
              },
