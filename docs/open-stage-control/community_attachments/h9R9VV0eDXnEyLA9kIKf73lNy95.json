        "ignoreDefaults": false,
        "bypass": true,
        "onCreate": "const len = getProp(this, 'quantity')\nlocals.drumPadMap = Array(len).fill(0).map((v, i) => v + 24 + i)\n\nset(\"drum_pad_map\", locals.drumPadMap)",
        "onValue": "const note = get(\"note_to_map\")\nconst padId = id.split(\"/\")[1]\nif (get(\"map_note\") && note !== \"none\") {\n  locals.drumPadMap[padId] = note\n  set(\"note_to_map\", \"none\")\n  set(\"map_note\", 0)\n  return\n}\nconst address = getProp(this, \"address\")\nconst preArgs = getProp(this, \"preArgs\")\nconst target = getProp(this, \"target\")\nif (value[padId] > 0) send(target, address, preArgs, locals.drumPadMap[padId], value[padId])\nelse send(target, address, preArgs, locals.drumPadMap[padId], 0)\n",
