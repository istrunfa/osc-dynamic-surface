            "onCreate": "globals.wrapDifference = function(a, wrap) {\n  var len = a.length\n  var newValues = Array(len).fill(0)\n  for(let i = 0; i < len; i++) {\n     newValues[i] = a[i] - wrap\n  }\n  set(\"xyLast\", newValues)\n}\n\nglobals.nemodulo = function(a,b) {\n  return a - (b * Math.floor(a / b))\n}\n\nglobals.abWrap = function(mode,start,val,depth) {\n\nvar out = start + (val * depth)\n\n  if(mode === 1) {\n    return out\n  }\n\n  if(mode === 2) {\n  \n    if(out >= 1 ) {\n      out = (1 - out) + 1\n    } \n    else if (out <= 0) {\n      out *= -1\n    }\n    return out\n  }\n\n  if(mode === 3) {\n    out = globals.nemodulo(0.999 * out, 1)\n    return out\n  }\n}",
