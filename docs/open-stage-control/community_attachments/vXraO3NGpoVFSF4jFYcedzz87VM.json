            "id": "funcRandomize",
            "comments": "",
            "value": "",
            "default": "",
            "linkId": "",
            "address": "auto",
            "preArgs": "",
            "typeTags": "",
            "decimals": 2,
            "target": "",
            "ignoreDefaults": false,
            "bypass": false,
            "onCreate": "globals.arraySum = function(a) {\n  var sum = 0;\n  for (var i in a) {\n    sum += a[i];\n  }\n  return sum\n}\n\nglobals.getEnabled = function(a) {\n  \n  var enabled = 0\n  var len = a.length\n  \n  for (let i = 0; i < len; i++) {\n    \n    if(a[i] === 1) {\n      enabled = 1;\n      break;\n    }\n  }\n  return enabled\n}\n\nglobals.valrange = function(val, min, max) {\n  return val * (max - min) + min\n}\n\nglobals.abrandom = function(a, amount) {\n  \n  var rndVal = (a + globals.valrange(Math.random(),-1, 1) * Math.pow(amount, 2) )\n  \n  rndVal = Math.abs(rndVal)\n  \n  if  (rndVal >= 1) {\n    rndVal = 1 - (Math.random() * amount)\n  }\n  \n  else if (rndVal <= 0) {\n    rndVal = rndVal + (Math.random() * amount)\n  }\n  return rndVal\n}",
