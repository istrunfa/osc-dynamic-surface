            "ignoreDefaults": false,
            "bypass": false,
            "onCreate": "// little mod taken from an example by > Jean Emmanuel\n\nglobals.ramp = function(btnValue, from, to, dur) {\n\n    var fps = 60\n       \n    if (btnValue) {\n      // start fade when button is on\n\n      var start = Date.now()\n\n      setInterval(function() {\n        var pos = (Date.now() - start) / dur\n        if (pos >= 1) {\n          clearInterval()\n          pos = 1\n          set(\"btnRand\", 0)\n        }\n        \n        var val  = pos * (to - from) + from\n        set(\"cvarRamp\", val)\n        set(\"faProgBar\", val)\n        \n      }, 1000 / fps)\n\n    } else {\n      // cancel when button is off ?\n      clearInterval()\n    }\n}",
            "onValue": "",
