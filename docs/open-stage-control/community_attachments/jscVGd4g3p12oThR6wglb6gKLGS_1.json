            "id": "MCU Sysex IN",
            "keyBinding": "",
            "keyRepeat": true,
            "keyType": "keydown",
            "value": "",
            "default": "",
            "linkId": "",
            "address": "/sysex",
            "preArgs": "",
            "typeTags": "",
            "decimals": 2,
            "target": "midi:Mackie",
            "ignoreDefaults": false,
            "bypass": false,
            "comments": "",
            "onValue": "locals.text = locals.text || Array(112).fill(\" \") // persistent variable to store full text\n\nif (value.includes(\"f0 00 00 66 14 12\")) { // sysex header for mackie lcd text\n\nvar d = value.split(\" \").slice(6).map(x=>parseInt(x, 16)), // hex to int\n    pos = d[0], // first byte -> position\n    text = d.slice(1).map(x=>String.fromCharCode(x)) // rest -> updated characters\n\ntext.pop() // drop sysex closing byte\n\n//Send Midi note on 50 to activate correct screen on MCU\nsend('midi:Mackie', '/note', 1 , 50 , 127);\n\n// update characters\nfor (var i = 0; i < text.length; i++) {\n  locals.text[i + pos] = text[i] // update \n}\n\n\n// update text widget croping start space and \"\n//set(\"lcd_MODIFIED\", locals.text.slice(9,39).join(\"\").replace('\"','').trimLeft().replace('\"',''))\nlet text2 = locals.text.slice(9,43).join(\"\").replace('\"','').trimLeft().replace('\"','').trimRight();\nlet words = text2.split(\" \");\nwords.pop();\nlet newLCD = words.join(\" \");\nlet TrackNoEndSpace = newLCD.trimRight();\nlet TrackSplit = TrackNoEndSpace.split(\" \");\n//set(\"Trackname\", TrackNoEndSpace);\nset(\"Trackname\", text2);\n\nlet test = newLCD.split(\" \");\nconsole.log(text2);\nconsole.log(words);\n//console.log(newLCD);\n//console.log(TrackSplit);\n\n\n\n}\n\n",
